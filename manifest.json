{
  "name": "Analyse It Screenshot Analyzer",
  "version": "1.11",
  "description": "Analyzes screenshots of the active tab using AI. No AI API needed. ",
  "manifest_version": 3,
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["https://www.google-analytics.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons8-take-screenshot-100.png",
      "32": "icons8-take-screenshot-100.png",
      "48": "icons8-take-screenshot-100.png",
      "128": "icons8-take-screenshot-100.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons8-take-screenshot-100.png",
    "32": "icons8-take-screenshot-100.png",
    "48": "icons8-take-screenshot-100.png",
    "128": "icons8-take-screenshot-100.png"
  }
}
